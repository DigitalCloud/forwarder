{
    "name": "digitalcloud/laravel-forwarder",
    "description": "Laravel package allows you to fetch data from external link and return it to a callback url",
    "keywords": [
        "laravel",
        "package",
        "forwarder",
        "proxy",
        "request",
        "response",
        "manager"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalCloud\\Forwarder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCloud\\Forwarder\\ForwarderServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
